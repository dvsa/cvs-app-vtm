{
  "name": "cvs-app-vtm",
  "version": "0.0.1",
  "author": "https://ionicthemes.com",
  "contributors": [
    "Arup Banerjee <arupalan@gmail.com>"
  ],
  "homepage": "https://ionicthemes.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --no-cache --testMatch=\"**/ngx-data-table-datasource.spec.ts\" --coverage",
    "lint": "ng lint",
    "e2e": "cypress open",
    "test:deploy": "npm run build && npx http-server ./dist/cvs-app-vtm -p 4200 -c-1",
    "prepush": "npm run tslint && npm run build && npm test",
    "tslint": "tslint src/**/*.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~6.1.1",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "~6.1.1",
    "@angular/core": "~6.1.1",
    "@angular/forms": "~6.1.1",
    "@angular/http": "~6.1.1",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "~6.1.1",
    "@angular/platform-browser-dynamic": "~6.1.1",
    "@angular/router": "~6.1.1",
    "@azure/msal-angular": "^0.1.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@ionic-native/core": "5.0.0-beta.15",
    "@ionic-native/splash-screen": "5.0.0-beta.15",
    "@ionic-native/status-bar": "5.0.0-beta.15",
    "@ionic/angular": "4.0.0-beta.5",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "c-coffee-icon": "0.0.2",
    "core-js": "^2.5.3",
    "govuk-frontend": "^3.3.0",
    "hammerjs": "^2.0.8",
    "husky": "^3.0.9",
    "install": "^0.13.0",
    "jest-preset-angular": "^7.1.1",
    "microsoft-adal-angular6": "^1.3.0",
    "ngx-json-viewer": "^2.4.0",
    "npm": "^6.11.3",
    "rxjs": "6.2.2",
    "rxjs-compat": "^6.5.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.7.2",
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular-devkit/core": "~0.7.2",
    "@angular-devkit/schematics": "~0.7.2",
    "@angular/cli": "~6.1.1",
    "@angular/compiler": "~6.1.1",
    "@angular/compiler-cli": "~6.1.1",
    "@angular/language-service": "~6.1.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@ionic/ng-toolkit": "^1.0.0",
    "@ionic/schematics-angular": "^1.0.0",
    "@ngrx/store": "^6.1.0",
    "@types/jest": "^24.0.19",
    "@types/node": "~10.9.2",
    "codelyzer": "~4.4.2",
    "cypress": "^3.4.1",
    "jest": "^24.9.0",
    "jest-plugin-context": "^2.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/src/test.ts"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic)"
    ],
    "moduleNameMapper": {
      "^@ionic/angular": "<rootDir>/node_modules/@ionic/angular/dist/index.js",
      "^@ionic/core/loader": "<rootDir>/node_modules/@ionic/core/dist/esm/es5/ionic.define.js",
      "^@ionic/core/dist/ionic/svg": "<rootDir>/scripts/svgoverride.js"
    },
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": true
                },
                "modules": "commonjs"
              }
            ]
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import"
          ]
        },
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "<rootDir>/node_modules/jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepush",
      "pre-push": "npm run prepush"
    }
  },
  "description": "An Ionic project showcasing Angular custom components and Stencil custom web components"
}
