    ng-reflect-adr-details="[object Object]"
    ng-reflect-edit="true"
  >
    
    
    
    
    <vtm-tank-details-edit
      ng-reflect-tank="[object Object]"
      ng-reflect-vehicle-type="centre axle battery"
    >
